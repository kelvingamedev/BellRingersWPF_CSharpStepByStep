<Window x:Class="BellRingers.About"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:BellRingers"
        mc:Ignorable="d" Name="AboutBellRingers"
        Title="About Middleshire Bell Ringers" Height="156" Width="300" ResizeMode="NoResize">
    <Grid>
        <Label Height="28" Content="Version: 1.0" HorizontalAlignment="Left" Margin="80,20,0,0" Name="version" VerticalAlignment="Top" Width="75"/>
        <Label Content="Build date: September 2009" Height="28" Width="160" Name="buildDate" HorizontalAlignment="Left" Margin="80,50,0,45"/>
        <Button x:Name="button" Content="OK" Height="23" HorizontalAlignment="Left" Margin="100,85,0,0" VerticalAlignment="Top" Width="78" Click="button_Click"/>

    </Grid>
</Window>
